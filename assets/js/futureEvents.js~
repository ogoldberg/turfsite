 $(document).ready(function(){
                    $.ajax({
                        url: 'http://localhost:8080/turf/event/futureEvents',
                        dataType: 'jsonp',
                        success: function(data)  {
                            alert("Yikes!");
                            $("#futureEvents").html(data.result.futureEvents)
                        }
                        });
                    });
